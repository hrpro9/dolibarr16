
    if ((!function_exists('calculateACapitaN')) || (!function_exists('calculateACapitaN1')) || (!function_exists('calculateACapitaN2')) ) {
      function calculateACapitaN($dateCreation, $anneeDebut, $variableEx,$codecompt) {
        $valeurn = ($dateCreation == $anneeDebut) ? montantCalcul($codecompt, $anneeDebut) : $variableEx;
        return $valeurn;
      }
      function calculateACapitaN1($dateCreation, $dateCreationN1, $variableEx,$codecompt) {
        $valeurn1 = ($dateCreation == $dateCreationN1) ? montantCalcul($codecompt, $dateCreationN1) : $variableEx;
        return $valeurn1;
      }
      function calculateACapitaN2($dateCreation, $dateCreationN2, $variableEx,$codecompt) {
        $valeurn2 = ($dateCreation == $dateCreationN2) ? montantCalcul($codecompt, $dateCreationN2) : $variableEx;
        return $valeurn2;
      }
    }

  
  
 
  

  


   
    
    switch ($numero_compte) {
        case "514":
        $aCapita = calculateACapitaN($dateCreation, $anneeDebut, $aCapita,'514');
        $aCapitaN1 = calculateACapitaN1($dateCreation, $dateCreationN1, $aCapitaN1,'514');
        $aCapitaN2 = calculateACapitaN2($dateCreation, $dateCreationN2, $aCapitaN2,'514');
        break;
