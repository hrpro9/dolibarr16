<?php


    // Load Dolibarr environment
    require '../../main.inc.php';
    require_once '../../vendor/autoload.php';
    require_once DOL_DOCUMENT_ROOT . '/core/class/html.formfile.class.php';
    require_once DOL_DOCUMENT_ROOT . '/core/class/html.formother.class.php';
    require_once DOL_DOCUMENT_ROOT . '/core/lib/date.lib.php';

    llxHeader("", "");
    
    $fraisP_brut=0; //Frais préliminaires_brut
    $fraisP_Amort_prov=0; //Frais préliminaires_Amort_prov
    $fraisP_net=0;//Frais préliminaires_net
    $fraisP_EP=0; //Frais Préliminaires Exercice Precedent
    $chargesR_brut=0; //Charges à repartir sur plusieurs exercices_brut
    $chargesR_Amort_prov=0; //Charges à repartir sur plusieurs exercices_Amort_prov
    $chargesR_net=0;//Charges à repartir sur plusieurs exercices_net
    $chargesR_EP=0; //ChargesRepartir Exercice Precedent
    $primesR_brut=0; //Primes de remboursement des obligations_brut
    $primesR_Amort_prov=0; //Primes de remboursement des obligations_Amort_prov
    $primesR_net=0; //Primes de remboursement des obligations_net
    $primesR_EP=0; //PrimesRemboursement Exercice Precedent
    $immReche_brut=0; //Immobilisations en recherche et développement	_brut
    $immReche_Amort_prov=0;//Immobilisations en recherche et développement_Amort_prov
    $immReche_Amort_prov1=0;//Immobilisations en recherche et développement_Amort_prov1
    $immReche_Amort_prov2=0;//Immobilisations en recherche et développement_Amort_prov2
    $immReche_net=0; //Immobilisations en recherche et développement_net
    $immReche_EP=0;//Immobilisations en recherche et développement Exercice Precedent
    $BMD_brut=0; //Brevets, marques, droits et valeurs similaires_brut
    $fondsC_brut=0; //Fonds commercial_brut	
    $autresImmoInc_brut=0; //Autres immobilisations incorporelles_brut
    $terrains_brut=0; //Terrains_brut
    $cons_brut=0; //Constructions_brut
    $instalTechMat_brut=0; //Installations techniques, matériel et outillage_brut
    $matTransp_brut=0; //Matériel de transport_brut
    $mobMatAmenag_brut=0; //Mobiliers, matériel de bureau et aménagements divers_brut
    $autresImmoCor_brut=0; //Autres immobilisations corporelles_brut
    $immCorEnCours_brut=0; //Immobilisations corporelles en cours Brut
    $immNonVal_brut=0; //IMMOBILISATION EN NON VALEUR_brut
    $immNonVal_Amort_prov=0; //IMMOBILISATION EN NON VALEUR_Amort_prov
    $immNonVal_net=0; //IMMOBILISATION EN NON VALEUR_net
    $immNonVal_EP=0; //IMMOBILISATION EN NON VALEUR Exercice Precedent
    $immIncor_brut=0; //IMMOBILISATIONS INCORPORELLES_brut
    $immCor_brut=0; //IMMOBILISATIONS CORPORELLES_brut

    
    if(isset($_POST['chargement'])){
      $dateChoisis=$_POST['date_select'];
     }

    function affichageAnnees() {
      $anneeDebut = date('Y');
      $anneeFin = 2015;
      
      for ($annee = $anneeDebut; $annee >= $anneeFin; $annee--) {
        
          echo '<option value="' . $annee . '">' . $annee . '</option>';

      }
     }
     function readMontant($name){
      $name=!empty($name)?$name:0;
      echo number_format($name,2);
     }

     function montanttest($code_compt)
      {
        global $db; // Assurez-vous que la variable $db est accessible dans cette fonction
        
        $sql = "SELECT SUM(montant) AS montant FROM llx_accounting_bookkeeping WHERE numero_compte LIKE '$code_compt%'";
        $rest = $db->query($sql);
        $row = ((object)($rest))->fetch_assoc();
        $mont = 0; // Initialisez la variable $mont pour éviter une erreur si aucune valeur n'est trouvée
        
      
          $mont = $row['montant'];
        
        
        return $mont;
      }


     
      
     $sql ="SELECT * FROM llx_accounting_bookkeeping ";
     $result =$db->query($sql);
     foreach($result as $row){

      if(!empty($dateChoisis)){
        $anneeDebut = $dateChoisis;
      }else{

        $anneeDebut = date('Y');
      }

        // dpai
        $datetime = new DateTime($row['date_creation']);
        $dateCreation = $datetime->format('Y');



      if($dateCreation==$anneeDebut)
      {
      
        $numero_compte=$row['numero_compte'];
        $montant=$row['montant'];

        switch($numero_compte){
          case '345': 
           
              $fraisP_brut=montanttest('345');
            
            break;
          case '212': $chargesR_brut=$montant;break;
          case '213': $primesR_brut=$montant;break;
          case '2811': $fraisP_Amort_prov=$montant;break;
          case '2812': $Charges_a_repartirAmort_prov=$montant;break;
          case '2813': $primesR_Amort_prov=$montant;break;
  
          case '221': $immReche_brut=$montant;break;
          case '222': $BMD_brut=$montant;break;
          case '223': $fondsC_brut	=$montant;break;
          case '228': $autresImmoInc_brut=$montant;break;
          case '2821': $immReche_Amort_prov1=$montant;break;
          case '2891': $immReche_Amort_prov2=$montant;break;
  
          case '231': $terrains_brut=$montant;break;
          case '232': $cons_brut=$montant;break;
          case '233': $instalTechMat_brut=$montant;break;
          case '234': $matTransp_brut=$montant;break;
          case '235': $mobMatAmenag_brut=$montant;break;
          case '238': $autresImmoCor_brut=$montant;break;
          case '239': $immCorEnCours_brut=$montant;break;
  
          default:$default=$montant;break;
  
        }
      }


      





      
     }
 
     // calcule Frais Préliminaires Net
     $fraisP_net=$fraisP_brut-$fraisP_Amort_prov;
     // calcule Charges a repartir Net
     $chargesR_net=$chargesR_brut-$chargesR_Amort_prov;
     // calcule Primes de remboursement Net
     $primesR_net=$primesR_brut-$primesR_Amort_prov;
     // calcule Immobilisations en recherche Amort and prov
     $immReche_Amort_prov=-$immReche_Amort_prov1-$immReche_Amort_prov2;
     // calcule Immobilisations en recherche Net
     $immReche_net=$immReche_brut-$immReche_Amort_prov;
 
     // calcule IMMOBILISATION EN NON VALEUR brut/amort and prov/net/Exercice precedent/Exercice N-2
     $immNonVal_brut=$fraisP_brut+$chargesR_brut+$primesR_brut;
     $immNonVal_Amort_prov=$fraisP_Amort_prov+$chargesR_Amort_prov+$primesR_Amort_prov;
     $immNonVal_net=$fraisP_net+$chargesR_net+$primesR_net;
     $immNonVal_EP=$fraisP_EP+$chargesR_EP+$primesR_EP;

     // calcule IMMOBILISATIONS INCORPORELLES brut/amort and prov/net
     $immIncor_brut=$immReche_brut+$BMD_brut+$fondsC_brut+$autresImmoInc_brut;

     // calcule IMMOBILISATIONS_CORPORELLES brut/amort and prov/net
     $immCor_brut=$terrains_brut+$cons_brut+$instalTechMat_brut+$matTransp_brut+
     $mobMatAmenag_brut+$autresImmoCor_brut+$immCorEnCours_brut;
 
     // calcule Exercice precedent Frais de préliminaires
     $fraisP_EP=$fraisP_brut+$fraisP_Amort_prov;
     // calcule Exercice precedent Charges a repartir
     $chargesR_EP=$chargesR_brut+$chargesR_Amort_prov;
     
     // calcule Exercice precedent Primesde remboursement
     $primesR_EP=$primesR_brut+$primesR_Amort_prov;
     // calcule Exercice precedent Immobilisations en recherche
     $immReche_EP=$immReche_brut+$immReche_Amort_prov1+$immReche_Amort_prov2;
     


    

